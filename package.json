{
    "name": "latte-carousel",
    "version": "1.6.1",
    "description": "LatteCarousel is a lightweight and responsive carousel.",
    "repository": "https://github.com/latte-carousel/latte-carousel.git",
    "author": {
        "name": "LatteCarousel",
        "url": "https://latte-carousel.github.io"
    },
    "license": "MIT",
    "private": false,
    "main": "dist/latte-carousel.min.js",
    "types": "dist/lib/index.d.ts",
    "keywords": [
        "carousel",
        "responsive",
        "javascript",
        "slider",
        "typescript"
    ],
    "homepage": "https://lattecarousel.dev",
    "scripts": {
        "build": "webpack",
        "watch": "webpack --watch",
        "serve": "http-server \"./\"",
        "test": "jest",
        "format": "yarn run format:prettier && yarn run format:csscomb",
        "format:prettier": "prettier --find-config-path --write \"./src/**\" \"./test/**\" \"./example/**\"",
        "format:csscomb": "csscomb \"./src/scss\" \"./example\"",
        "lint": "yarn run lint:tslint && yarn run lint:eslint",
        "lint:tslint": "tslint -c \"tslint.json\" \"./**/*.ts\"",
        "lint:eslint": "eslint \"./**/*.js\""
    },
    "devDependencies": {
        "@types/jest": "^25.2.1",
        "autoprefixer": "^9.7.6",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.5.2",
        "csscomb": "^4.3.0",
        "cssnano": "^4.1.10",
        "eslint": "^6.8.0",
        "http-server": "^0.12.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^25.4.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.14.0",
        "postcss-loader": "^3.0.0",
        "prettier": "2.0.5",
        "sass-loader": "^8.0.2",
        "ts-jest": "^25.4.0",
        "ts-loader": "^7.0.1",
        "tslint": "^6.1.1",
        "typescript": "^3.8.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    }
}
